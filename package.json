{
    "name": "etsoo-react",
    "version": "1.0.39",
    "description": "ETSOO TypeScript React components NPM package",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write src/**/*.{ts,tsx}",
        "lint": "eslint --ext .ts,.tsx src/",
        "test": "jest"
    },
    "@comments": {
        "jest configuration help": "https://basarat.gitbook.io/typescript/intro-1/jest"
    },
    "jest": {
        "testMatch": [
            "<rootDir>/__tests__/**/*.{ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            ".+\\.jsx?$": "babel-jest",
            ".+\\.tsx?$": "ts-jest"
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!@etsoo/).+\\.js$"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ETSOO/etsoo-react.git"
    },
    "keywords": [
        "TypeScript",
        "Material-UI",
        "SmartERP"
    ],
    "author": "Garry Xiao",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ETSOO/etsoo-react/issues"
    },
    "homepage": "https://github.com/ETSOO/etsoo-react#readme",
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.11.0",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.9",
        "@types/react": "^16.9.45",
        "@types/react-dom": "^16.9.8",
        "@types/react-input-mask": "^2.0.5",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-window": "^1.8.2",
        "@types/react-window-infinite-loader": "^1.0.3",
        "@types/yup": "^0.29.4",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^26.3.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.3",
        "eslint": "^7.6.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-react": "^7.20.5",
        "jest": "^26.2.2",
        "jest-fetch-mock": "^3.0.3",
        "prettier": "^2.0.5",
        "react-test-renderer": "^16.13.1",
        "regenerator-runtime": "^0.13.7",
        "ts-jest": "^26.1.4",
        "node-fetch": "^2.6.1"
    },
    "dependencies": {
        "@etsoo/notificationmu": "^1.0.12",
        "@etsoo/restclient": "^1.0.25",
        "@etsoo/shared": "^1.0.6",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@types/node": "^14.0.27",
        "history": "^5.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-draggable": "^4.4.3",
        "react-input-mask": "^2.0.4",
        "react-router-dom": "^5.2.0",
        "react-svg": "^11.0.36",
        "react-window": "^1.8.5",
        "react-window-infinite-loader": "^1.0.5",
        "typescript": "^3.9.7",
        "yup": "^0.29.3"
    }
}
